{
  "name": "Constraints Blocker",
  "description": "Build an Extension!",
  "version": "1.0.0.1",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "LeetCode_logo_black.png",
      "32": "LeetCode_logo_black.png",
      "48": "LeetCode_logo_black.png",
      "128": "LeetCode_logo_black.png"
    }
  },
  "icons": {
    "16": "LeetCode_logo_black.png",
    "32": "LeetCode_logo_black.png",
    "48": "LeetCode_logo_black.png",
    "128": "LeetCode_logo_black.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.binarysearch.com/*"
      ],
      "js": [
        "binarysearch.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.leetcode.com/*"
      ],
      "js": [
        "leetcode.js"
      ],
      "run_at": "document_end"
    }
  ],
  "offline_enabled": true
}